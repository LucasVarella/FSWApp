<?xml version='1.0' encoding='utf-8'?>
<widget id="es.esy.profvictorhugo.AppFSW" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>FSWApp</name>
    <description>
        Aplicativo de acesso aos recursos da Fábrica de Software Acadêmica, do Câmpus Inhumas do IFG.
    </description>
    <author email="elymar.cabral@ifg.edu.br" href="http://fsw.ifg.edu.br/app">
        Equipe FSW Acadêmica - Coordenação Prof. Elymar P. Cabral
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <splash src="res/screen/android/screen-mdpi-portrait.png" />
    <gap:splash src="res/screen/android/screen-mdpi-portrait.png" />
    <icon density="ldpi" src="res/icon/android/ldpi.png" />
    <icon density="mdpi" src="res/icon/android/mdpi.png" />
    <icon density="hdpi" src="res/icon/android/hdpi.png" />
    <icon density="xhdpi" src="res/icon/android/xhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="SplashScreenDelay" value="10000" />
    <preference name="Orientation" value="portrait" />
    <preference name="Fullscreen" value="true" />
</widget>
